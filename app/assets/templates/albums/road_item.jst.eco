<div class="road">
  <div class="place-header">
    <div class="place-name">
      <%= "On the road, #{@place_from.get('name')} to #{@place_to.get('name')}" %>
    </div>
  </div>
  <div class="section-header">
    Means of transport
  </div>
  <ul class="place-buttons transport">
    <li>
      <a href="0" class="train">
        <span class="content-icon train"></span>
        <div>
          By train
        </div>
      </a>
    </li>
    <li>
      <a href="1" class="car">
        <span class="content-icon car"></span>
        <div>
          By car
        </div>
      </a>
    </li>
    <li>
      <a href="2" class="plane">
        <span class="content-icon plane"></span>
        <div>
          By plane
        </div>
      </a>
    </li>
    <li>
      <a href="3" class="hitchhike">
        <span class="content-icon hitchhike"></span>
        <div>
          Hitchhiking
        </div>
      </a>
    </li>
    <li>
      <a href="4" class="foot">
        <span class="content-icon shoe"></span>
        <div>
          By foot
        </div>
      </a>
    </li>
    <li>
      <a href="5" class="bike">
        <span class="content-icon bike"></span>
        <div>
          By bike
        </div>
      </a>
    </li>
  </ul>
  <div class="section-header">
    Upload Content
  </div>
  <ul class="place-buttons">
    <li>
      <a href="#" class="photo">
        <span class="content-icon photo"></span>
        <div>
          Photo
        </div>
      </a>
    </li>
    <li>
      <a href="#" class="video disabled">
        <span class="content-icon video"></span>
        <div>
          Video
        </div>
      </a>
    </li>
    <li>
      <a href="#" class="music disabled">
        <span class="content-icon music"></span>
        <div>
          Music
        </div>
      </a>
    </li>
    <li>
      <a href="#" class="story disabled">
        <span class="content-icon story"></span>
        <div>
          Story
        </div>
      </a>
    </li>
    <li>
      <a href="#" class="others disabled">
        <span class="content-icon others"></span>
        <div>
          Other stuff
        </div>
      </a>
    </li>
  </ul>
</div>
